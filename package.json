{
  "name": "kremen-transport-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "data:update": "curl https://api.kremen.dev/transport/routes --output src/core/data/routes.json && curl https://api.kremen.dev/transport/buses --output src/core/data/buses.json",
    "test": "jest",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:types": "tsc --noEmit --skipLibCheck",
    "lint:eslint": "eslint \"src/**\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.14",
    "color": "^4.1.0",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-code-push": "7.0.4",
    "react-native-config": "1.4.5",
    "react-native-maps": "0.29.4",
    "react-native-svg": "12.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@types/color": "3.0.2",
    "@types/jest": "^26.0.23",
    "@types/lodash": "4.14.177",
    "@types/react-native": "^0.66.4",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "babel-jest": "^26.6.3",
    "eslint": "8.3.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
